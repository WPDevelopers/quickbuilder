{
    "compilerOptions": {
        "strict": true,
        // "declaration": true,
        "target": "es5",
        "lib": [
            "dom",
            "es2015"
        ],
        "jsx": "react",
        "module": "esnext", // "esnext",
        "esModuleInterop": true,
        "allowJs": false, // false
        "allowSyntheticDefaultImports": true,
        "noImplicitAny": false,
        "moduleResolution": "node",
        "noUnusedLocals": false,
        "sourceMap": false, // true
        "types": ["node"],
    },
    "include": [
        "src/**/*",
        // "src/fields/index.ts",
      ],
}
